{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Funds Agentic (module, src on PYTHONPATH)",
            "type": "debugpy",
            "request": "launch",
            "module": "funds_agentic.main",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src",
                "HEADLESS": "0", // force headed
                "PWDEBUG": "1", // if Playwright
                "PLAYWRIGHT_VERBOSE": "1" // optional verbose logs
            },
            "args": [
                "--output",
                "G:/My Drive/Investments/scraper",
                "--input",
                "G:/My Drive/Investments/Holdings/AllHoldings_Updated.xlsx",
                "--graph-out",
                "G:/My Drive/Investments/Github/funds-agentic/workspace/graph/graph.mmd",
                "--graph-format", "mermaid"
            ],
            "justMyCode": true
        }
    ]
}
